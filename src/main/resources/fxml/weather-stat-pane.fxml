<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="containerGrid" xmlns:fx="http://javafx.com/fxml/1"
          xmlns="http://javafx.com/javafx/8.0.112-ea">
    <columnConstraints>
        <ColumnConstraints percentWidth="40" />
        <ColumnConstraints percentWidth="60" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints percentHeight="50" />
        <RowConstraints percentHeight="50" />
    </rowConstraints>
    <StackPane fx:id="imageContainer" styleClass="weather-icon-container2"
               GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.rowSpan="2">
    </StackPane>
</GridPane>